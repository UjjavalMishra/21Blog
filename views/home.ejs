<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/link') %>
</head>
<body>
    <%- include('./partials/nav') %>
    <div class="container">
        <div class="container mt-3 row row-cols-3" style="display: flex; justify-content: space-between;">
            <% blogs.forEach(blog => { %>
               <div class="col mt-5">
                <div class="card col" style="width: 18rem;">
                    <img src="<%= blog.coverImageURL  %>" class="card-img-top" alt="...">
                    <div class="card-body">
                      <h5 class="card-title"><%= blog.title  %></h5>
                      <a href="#" class="btn btn-primary">Open</a>
                    </div>
                  </div>
               </div>
            <% }) %>
        </div>
    </div>
    <%- include('./partials/script') %>
</body>
</html>